{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "ASSEMBLY_AI_API_KEY": "@assembly_ai_key",
    "ELEVEN_LABS_API_KEY": "@eleven_labs_key",
    "SECRET_KEY": "@secret_key"
  }
}